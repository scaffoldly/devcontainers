{
    "id": "aws-localstack",
    "version": "1.0.1",
    "name": "AWS (Localstack)",
    "description": "AWS Localstack Devcontainer",
    "documentationURL": "https://github.com/scaffoldly/devcontainers/tree/main/src/aws",
    "licenseURL": "https://github.com/scaffoldly/devcontainers/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Ubuntu version:",
            "proposals": [
                "jammy"
            ],
            "default": "jammy"
        },
        "pythonVariant": {
            "type": "string",
            "description": "Python version:",
            "proposals": [
                "3.10"
            ],
            "default": "3.10"
        },
        "localstackVariant": {
            "type": "string",
            "description": "Localstack version:",
            "proposals": [
                "1.4"
            ],
            "default": "1.4"
        }
    },
    "platforms": [
        "Any"
    ]
